#!/bin/bash

# Notice how this is not copied by the copy script, you have to do
# this manually as this could just be a sample and is not often
# changed
# This is just a sample

export PATH="$PATH:$HOME/Dev/flutter/bin/"
export PATH="$PATH":"$HOME/Dev/flutter/.pub-cache/bin"
export PATH="$PATH":"$HOME/Dev/flutter/bin/cache/dart-sdk/bin/"


# IBM specific config
export everyone='/gsa/tlbgsa/home/k/a/kaipada/everyone' # accessible on all machines
export run_temp='/gsa/tlbgsa/home/k/a/kaipada/temp_script' # For quick and dirty scripting on the go, accessible on all machines
alias run_temp='/gsa/tlbgsa/home/k/a/kaipada/temp_script'
alias ll='ls -al' # h option is not supported on AIX or Z/OS
export TERM=xterm
set -o vi
PS1='\n\[\e[01;33m\]\u\[\e[0m\]\[\e[00;37m\]@\[\e[0m\]\[\e[01;36m\]\h\[\e[0m\]\[\e[00;37m\] \t \[\e[0m\]\[\e[01;35m\]\w\[\e[0m\]\[\e[01;37m\] \[\e[0m\]\n$ ' # for ease of copying
